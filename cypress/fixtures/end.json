"var current = -1;var name_element = document.getElementById('student_name');var link_element = document.getElementById('student_link');var page_element = document.getElementById('student_page');let next = function(){current++;if(current > students.length-1){current = 0;};name_element.innerHTML = students[current].name;link_element.innerHTML = students[current].link;page_element.src = students[current].link;};let prev = function(){current--;if(current < 0){current = students.length - 1;};name_element.innerHTML = students[current].name;link_element.innerHTML = students[current].link;page_element.src = students[current].link;};</script></body></html>"